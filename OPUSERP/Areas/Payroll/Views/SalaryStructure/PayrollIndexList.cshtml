            });

            $('.deductionTotal').text(totaldeduction);

            //NetTotal
            $('.tr').each(function () {

                var NetVal = $(this).find('.net_' + $(this).find('.td1').text()).text();
              //  console.log(NetVal);
                totalNet = parseFloat(NetVal) + parseFloat(totalNet);
            });

            $('.netTotal').text(totalNet);

        }

