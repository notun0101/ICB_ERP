@model ReportMasterViewModel
@{
    ViewData["Title"] = "RequisitionView";
    Layout = "~/Views/Shared/_PrintView.cshtml";
    decimal? vat = Model?.purchaseOrderRPTViewModels?.FirstOrDefault()?.vat;
    decimal? tax = Model?.purchaseOrderRPTViewModels?.FirstOrDefault()?.tax;
    decimal? subTotal = Model?.purchaseOrderRPTViewModels?.Sum(x => x.poValueItem);
    decimal? total = vat + subTotal + tax;
    string CopyStatus = string.Empty;
    if (Model.isCopy == 1)
    {
        CopyStatus = "Copy";
    }
}

@{
    ViewData["Title"] = "PurchaseOrderInfo";
}
