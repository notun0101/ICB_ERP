                </div>
            </nav>
        </div>
        @*<div class="row" style="height:10px;"></div>*@
        <div class="tab-content" id="nav-tabContent">
            @*<div id="loader" style="display: none; justify-content: center; align-items: center;">
                <img src="~/images/loader.gif" alt="Alternate Text" />
            </div>*@
            <div class="tab-pane fade show active" id="nav-1stPart" role="tabpanel" aria-labelledby="nav-1stPart-tab">
                @*<div class="row col-sm-12">*@
