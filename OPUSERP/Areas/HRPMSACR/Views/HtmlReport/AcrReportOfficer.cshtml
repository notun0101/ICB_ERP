    }

    .showcircle {
        height: 60px;
        width: 60px;
        -webkit-border-radius: 25px;
        -moz-border-radius: 25px;
        border-radius: 55px;
        border: 1px solid red;
        line-height: 55px;
        text-align: center;
    }

    .showcircleWithoutBorder {
        height: 60px;
        width: 60px;
        -webkit-border-radius: 25px;
        -moz-border-radius: 25px;
        border-radius: 55px;
        border: none;
        line-height: 55px;
        text-align: center;
    }

    .table td, .table th {
        font-size: 34px;
        padding: .3rem;
    }


