                                    <label for="empNameBn" class="col-sm-1 col-form-label">:</label>
                                    <div class="col-md-8 col-sm-8">
                                        <input type="text" name="empNameBn" id="empNameBn" value="@Model.ACRUsers?.EmpNameBn" class="form-control" readonly />
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <label class="col-md-3 col-sm-3" for="empNameEn">&nbsp; &nbsp;&nbsp; (ইংরেজী বড় হাতের অক্ষরে)</label>
                                    <label for="empNameEn" class="col-sm-1 col-form-label">:</label>
                                    <div class="col-md-8 col-sm-8">
                                        <input type="text" name="empNameEn" id="empNameEn" class="form-control" value="@Model.ACRUsers?.EmpName.ToUpper()" readonly />
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <label for="designationName" class="col-sm-3">(খ) পদবী</label>
                                    <label for="designationName" class="col-sm-1 col-form-label">:</label>
                                    <div class="col-sm-8">
                                        <input type="text" name="designationName" id="designationName" class="form-control" value="@Model.ACRUsers?.DesignationName" readonly />
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <label class="col-sm-3" for="presentOffice">(গ) বর্তমান কর্মস্থল</label>
                                    <label for="presentOffice" class="col-sm-1 col-form-label">:</label>
                                    <div class="col-sm-8">
                                        <input type="text" name="presentOffice" id="presentOffice" class="form-control" value="@Model.ACRUsers?.WorkStation" readonly />
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <label class="col-sm-3" for="fatherName">(ঘ) পিতার নাম</label>
                                    <label for="fatherName" class="col-sm-1 col-form-label">:</label>
                                    <div class="col-sm-8">
                                        <input type="text" name="fatherName" id="fatherName" class="form-control" value="@Model.ACRUsers?.FatherName" readonly />
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <label class="col-sm-3" for="motherName">(ঙ) মাতার নাম</label>
                                    <label for="motherName" class="col-sm-1 col-form-label">:</label>
                                    <div class="col-sm-8">
                                        <input type="text" name="motherName" id="motherName" class="form-control" value="@Model.ACRUsers?.MotherName" readonly />
                                    </div>
                                </div>
                                <div class="form-group row d-none">
                                    <label class="col-sm-3" for="spouseName">(চ) স্বামী/স্ত্রীর নাম</label>
                                    <label for="spouseName" class="col-sm-1 col-form-label">:</label>
                                    <div class="col-sm-8">
                                        <input type="text" name="spouseName" id="spouseName" class="form-control" value="@Model.ACRUsers?.SpouseName" readonly />
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <label class="col-sm-3" for="dateOfBirth">(চ) জন্ম তারিখ</label>
                                    <label for="dateOfBirth" class="col-sm-1 col-form-label">:</label>
                                    <div class="col-sm-8">
                                        <input type="text" name="dateOfBirth" id="dateOfBirth" class="form-control" value="@Convert.ToDateTime(Model.ACRUsers?.BirthDate).ToString("dd-MMM-yyyy")" readonly />
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <label class="col-sm-3" for="nationalId">(ছ) জাতীয় পরিচয়পত্রের নম্বর</label>
                                    <label for="nationalId" class="col-sm-1 col-form-label">:</label>
                                    <div class="col-sm-8">
                                        <input type="text" name="nationalId" id="nationalId" class="form-control" value="@Model.ACRUsers?.NID" readonly />
                                    </div>
                                </div>
                                <div class="form-group row">
                                    <label class="col-sm-3" for="joiningDateAndDesig">(জ) যোগদানের তারিখ ও পদবী</label>
