                                <div class="row">
                                    <div class="col-md-4 col-sm-4">
                                        <table class="table table-bordered">
                                            <thead>
                                                <tr>
                                                    <th colspan="2">প্রযোজ্য ঘরে অনুস্বাক্ষর দিন</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr>
                                                    <td>বিবাহিত</td>
                                                    <td>
                                                        @if (Model.ACRUsers?.MaritalStatus == "Married")
                                                        {
                                                            <img src="/@Model.ACRUsers?.SignUrl" id="married" height="40" width="180" class="img-circle" />
                                                        }
                                                        else
                                                        {
                                                            <img id="married" height="40" width="180" class="img-circle" />
                                                        }
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td>অবিবাহিত</td>
                                                    <td>
                                                        @if (Model.ACRUsers?.MaritalStatus == "Unmarried")
                                                        {
                                                            <img src="/@Model.ACRUsers?.SignUrl" id="unmarried" height="40" width="180" class="img-circle" />
                                                        }
                                                        else
                                                        {
                                                            <img id="unmarried" height="40" width="180" class="img-circle" />
                                                        }
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                    <div class="col-md-4 col-sm-4" style="text-align:center;">
                                        <img id="empImg" src="~/@Model.ACRUsers?.ImageUrl" style="width:120px;height:120px;" class="img-circle" />
                                    </div>
                                    <div class="col-md-4 col-sm-4">
                                        <table class="table table-bordered">
                                            <thead>
                                                <tr>
                                                    <th colspan="2">প্রযোজ্য ঘরে অনুস্বাক্ষর দিন</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr>
                                                    <td>নিঃসন্তান</td>
                                                    <td>
                                                        @if (Model.ACRUsers?.childrenCount == 0)
                                                        {
                                                            <img src="/@Model.ACRUsers?.SignUrl" id="childless" height="40" width="180" class="img-circle" />
                                                        }
                                                        else
                                                        {
                                                            <img id="childless" height="40" width="180" class="img-circle" />
                                                        }
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td>১ টি সন্তান</td>
                                                    <td>
                                                        @if (Model.ACRUsers?.childrenCount == 1)
                                                        {
                                                            <img src="/@Model.ACRUsers?.SignUrl" id="firstchild" height="40" width="180" class="img-circle" />
                                                        }
                                                        else
                                                        {
                                                            <img id="firstchild" height="40" width="180" class="img-circle" />
                                                        }
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td>২ টি সন্তান</td>
                                                    <td>
                                                        @if (Model.ACRUsers?.childrenCount == 2)
                                                        {
                                                            <img src="/@Model.ACRUsers?.SignUrl" id="secondchild" height="40" width="180" class="img-circle" />
                                                        }
                                                        else
                                                        {
                                                            <img id="secondchild" height="40" width="180" class="img-circle" />
                                                        }

                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td>২-এর অধিক সন্তান</td>
                                                    <td>
                                                        @if (Model.ACRUsers?.childrenCount > 2)
                                                        {
                                                            <img src="/@Model.ACRUsers?.SignUrl" id="morechild" height="40" width="180" class="img-circle" />
                                                        }
                                                        else
                                                        {
                                                            <img id="morechild" height="40" width="180" class="img-circle" />
                                                        }
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row mt-2 mb-2">
                            <div class="col-md-2">

                            </div>
                            <div class="col-md-8 text-center">
