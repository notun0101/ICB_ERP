                                    <div class="col-md-9 col-sm-9">
                                        <input type="text" name="designationAtJoining" id="designationAtJoining" class="form-control" value="@Model.ACRUsers?.JoiningDesignation" readonly />
                                    </div>
                                </div>
                                <br />
                                <p><b>০২। <u>শিক্ষাগত যোগ্যতা :</u></b></p>
                                <table class="table table-bordered" id="tblEducationalQualifications">
                                    <thead>
                                        <tr>
                                            <th>পরীক্ষার নাম</th>
                                            <th>গ্র্রুপ/বিষয়</th>
                                            <th>বিভাগ/শ্রেণী/সিজিপিএ</th>
                                            <th>পাসের সাল</th>
                                            <th>বোর্ড/বিশ্ববিদ্যালয়</th>
                                        </tr>
                                    </thead>
