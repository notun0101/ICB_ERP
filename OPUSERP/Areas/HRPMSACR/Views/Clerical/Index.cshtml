                                            {<i class="fa fa-check"></i>}
                                        </div>
                                        <div class="col-md-6">

                                        </div>
                                        <div class="col-md-6">
                                            ত্রৈমাসিক @if (Model.AssessmentBasic.actionType == "Quaterly")
                                            {<i class="fa fa-check"></i>}
                                        </div>
                                        <div class="col-md-6">

                                        </div>
                                        <div class="col-md-6">
                                            বিশেষ @if (Model.AssessmentBasic.actionType == "Special")
                                            {<i class="fa fa-check"></i>}
                                        </div>
                                        <div class="col-md-6">

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="card d-none">
