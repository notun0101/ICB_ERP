<div class="row clearfix">
    <div class="col-12">
        <div class="card mb-4">
            <!-- Card Header - Dropdown -->
            <div class="card-header py-3 d-flex flex-row align-items-center justify-content-between">
                <h6 class="m-0 font-weight-bold text-primary">Asset Register List</h6>
            </div>
            <!-- Card Body -->
            <div class="card-body">
                <div class="col-sm-12">
                    <table class="table table-bordered table-striped table-responsive" id="assetRegisterTable" style="width:100%;">
                        <thead>
                            <tr>
                                <th>SL#</th>
                                <th>Asset No</th>
                                <th>Asset Name</th>
                                <th>Asset Value</th>
                                <th>Unit Price</th>
                                <th>Quantity</th>
                                <th>Carring Cost</th>
                                <th>Additional Cost</th>
                                <th>QR Code</th>
                                <th>Action</th>
                            </tr>
                        </thead>
                        <tbody>
                            @{
                                int i = 1;
                                @foreach (var data in Model.assetRegistrations.Where(x => x.purchaseInfo.purchaseOrderMasterId == null))
                                {
                                    <tr>
                                        <td>@i</td>
                                        <td><span style="display:none">-@data.Id-</span>@data.assetNo</td>
                                        <td>@data.itemSpecification.specificationName</td>
                                        <td style="text-align:right;">@data.assetValue?.ToString("0,0.00")</td>
                                        <td style="text-align:right;">@data.unitPrice?.ToString("0,0.00")</td>
                                        <td>@data.quantity</td>
                                        <td style="text-align:right;">@data.carringCost?.ToString("0,0.00")</td>
                                        <td style="text-align:right;">@data.additionalCost?.ToString("0,0.00")</td>
                                        <td><qrcode content="@data.assetNo, @data.itemSpecification.specificationName, @data.assetValue?.ToString("0,0.00")" width="100" height="70"></qrcode><br /></td>
                                        <td>
                                            @*<button onclick="BarCodePrintOption('@data.assetNo')" title="Add Print Info" class="btn btn-sm btn-info"><i class="fa fa-info fa-fw"></i></button>&nbsp;
                                                <a class="btn btn-success" data-toggle="tooltip" title="Edit" href="/FAMSAssetRegister/AssetRegister/AssetRegister/?id=@data.Id"><i class="fa fa-edit"></i></a>*@
                                            <a class="btn btn-danger" title="Delete" href="javascript:void(0)" onclick="DeleteMaster(@data.purchaseInfo.Id)"><i class="fa fa-trash-alt"></i></a>
                                        </td>
                                    </tr>
                                    i++;
                                }
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

@section ScriptsAssetRegister{
    <script type="text/javascript">        
        $(document).ready(function () {
            $("#imagePath_Challan").change(function () {
                readURLChallan(this);
            });
            $("#imagePath_Warranty").change(function () {
                readURLWarranty(this);
            });
            $("#imagePath_Item_Font").change(function () {
                readURLItem_Font(this);
            });
            $("#imagePath_Item_Back").change(function () {
                readURLItem_Back(this);
            });
            $('#assetRegisterTable').DataTable();
            $("#purchaseDate").datepicker({ dateFormat: "dd-M-yy", showAnim: "scale", changeMonth: true, changeYear: true, yearRange: "2010:2030" }).datepicker("setDate", new Date());
            $("#receiveDate").datepicker({ dateFormat: "dd-M-yy", showAnim: "scale", changeMonth: true, changeYear: true, yearRange: "2010:2030" }).datepicker("setDate", new Date());
            $("#challanDate").datepicker({ dateFormat: "dd-M-yy", showAnim: "scale", changeMonth: true, changeYear: true, yearRange: "2010:2030" }).datepicker("setDate", new Date());
            $("#Wdate").datepicker({ dateFormat: "dd-M-yy", showAnim: "scale", changeMonth: true, changeYear: true, yearRange: "2010:2030" }).datepicker("setDate", new Date());
            if ('@Model.purchaseOrderMaster?.Id'> '0')
            {
                $("#supplierId").val(@Model.purchaseOrderMaster?.supplierId);
            }
            //$("#itemSpecificationId").change(function () {
            //    var id = $("#itemSpecificationId").val();
            //    Common.Ajax('GET', '/global/api/ItemSpecId/' + id, [], 'json', GetItemName);
            //})
            $("#periodId").change(function () {
                var id = $("#periodId").val();
                Common.Ajax('GET', '/global/api/DepriciationInfo/' + id, [], 'json', GetDepInfo);
            })

            $("#purchaseRate").change(function () {
                $("#purchaseRateR").val($("#purchaseRate").val());
            })
            $("#additionalCost").change(function () {
                var cost = $("#additionalCost").val();
                var qty = $("#quantity").val();
                var cost = cost / qty;
                $("#additionalCostR").val(cost);
            })
            $("#carringCost").change(function () {
                var cost = $("#carringCost").val();
                var qty = $("#quantity").val();
                var cost = cost / qty;
                $("#carringCostR").val(cost);
            })

            Common.Ajax('GET', '/global/api/GetAllFixedAssetItem', [], 'json', ajaxGetAllFixedAssetItem);

            $(".money").keydown(function (e) {
                // Allow: backspace, delete, tab, escape, enter and .
                if ($.inArray(e.keyCode, [46, 8, 9, 27, 13, 110, 190]) !== -1 ||
                    // Allow: Ctrl/cmd+A
                    (e.keyCode == 65 && (e.ctrlKey === true || e.metaKey === true)) ||
                    // Allow: Ctrl/cmd+C
                    (e.keyCode == 67 && (e.ctrlKey === true || e.metaKey === true)) ||
                    // Allow: Ctrl/cmd+X
                    (e.keyCode == 88 && (e.ctrlKey === true || e.metaKey === true)) ||
                    // Allow: home, end, left, right
                    (e.keyCode >= 35 && e.keyCode <= 39)) {
                    // let it happen, don't do anything
                    return;
                }
                // Ensure that it is a number and stop the keypress
                if ((e.shiftKey || (e.keyCode < 48 || e.keyCode > 57)) && (e.keyCode < 96 || e.keyCode > 105)) {
                    e.preventDefault();
                }
            });
        });

        function readURLChallan(input) {
