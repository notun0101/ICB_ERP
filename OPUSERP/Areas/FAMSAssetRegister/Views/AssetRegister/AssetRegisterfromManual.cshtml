        function ajaxGetAllFixedAssetItem(response) {
            var GetList = [];
            $.each(response, function (id, option) {
                var obj = new Object();
                obj.key = option.Id;
                obj.value = option.itemName;
                GetList[GetList.length] = obj;
            });
            $('#itemName').autocomplete({
