@model AssetRegistrationViewModel
@{
    ViewData["Title"] = "AssetRegisterfromManual";
    Layout = "~/Areas/FAMSAssetRegister/Views/Shared/_AssetRegisterForm.cshtml";
}
<div class="row clearfix">
    <div class="col-12">
        <div class="card mb-4">
            <!-- Card Header - Dropdown -->
            <div class="card-header py-3 d-flex flex-row align-items-center justify-content-between">
                <h6 class="m-0 font-weight-bold text-primary">Manual Purchase Info</h6>
                <a href="/FAMSAssetRegister/AssetRegister/AssetRegisterList" class="btn btn-facebook btn-sm pull-right">Back To Asset List</a>
            </div>
            <div class="card-body">
                <div class="container">
                    <form asp-area="FAMSAssetRegister" asp-controller="AssetRegister" asp-action="AssetRegisterfromManual" method="post" data-parsley-validate="parsley" enctype="multipart/form-data">
                        <div asp-validation-summary="All" class="text-danger"></div>
                        <div class="row">
                            <div class="row col-sm-12">
                                <div class="col-sm-6">
                                    <div class="form-group row">
                                        <input type="hidden" name="purchaseInfoId" id="purchaseInfoId" value="0" />
                                        <input type="hidden" name="purchaseId" id="purchaseId" value="@Model.purchaseOrderMaster?.Id" />
                                        <label for="supplierId" class="col-sm-4 col-form-label">Supplier Name <span style="color:red">*</span></label>
                                        <div class="col-sm-8">
                                            <select class="form-control" id="supplierId" name="supplierId" data-parsley-required="true">
                                                <option value="" id="proposalTypeId">Select Supplier</option>
                                                @foreach (var data in Model.organizations)
                                                {
                                                    <option value="@data.Id">@data.organizationName</option>
                                                }
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <label for="purchaseNo" class="col-sm-4 col-form-label">Purchase No </label>
                                        <div class="col-sm-8">
                                            <input type="text" class="form-control" name="purchaseNo" id="purchaseNo" value="@Model.purchaseOrderMaster?.poNo">
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <label for="purchaseDate" class="col-sm-4 col-form-label">Purchase Date <span style="color:red">*</span></label>
                                        <div class="col-sm-8">
                                            <input type="text" data-parsley-required="true" class="form-control" name="purchaseDate" id="purchaseDate" value="@Model.purchaseOrderMaster?.poDate">
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <label for="receiveDate" class="col-sm-4 col-form-label">Receive Date <span style="color:red">*</span></label>
                                        <div class="col-sm-8">
                                            <input type="text" data-parsley-required="true" class="form-control" name="receiveDate" id="receiveDate" value="@Model.PurchaseInfo?.receiveDate">
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <label for="challanNo" class="col-sm-4 col-form-label">Voucher No </label>
                                        <div class="col-sm-8">
                                            <input type="text" class="form-control" name="challanNo" id="challanNo" value="">
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <label for="challanDate" class="col-sm-4 col-form-label">Challan Date <span style="color:red">*</span></label>
                                        <div class="col-sm-8">
                                            <input type="text" data-parsley-required="true" class="form-control" name="challanDate" id="challanDate" value="@Model.purchaseOrderMaster?.deliveryDate">
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <label for="procurementSourceId" class="col-sm-4 col-form-label">Procurement Source<span style="color:red">*</span></label>
                                        <div class="col-sm-8">
