   function ajaxLoadLeaveApprovarNew(response) {
    var option = '';
    $.each(response, function (i, item) {
        option += `<tr>
							<td>${item.employeeInfo?.employeeCode}</td>
							<td>${item.employeeInfo?.nameEnglish}</td>
							<td>${item.approvalType?.approvalTypeName}</td>


							<td>
                               <a class="btn btn-success" onclick="EditApprovalTable(${item.Id}, ${item.approvalTypeId},${item.employeeInfoId}, '${item.employeeInfo?.nameEnglish}', '${item.employeeInfo?.lastDesignation?.designationName}', '${item.employeeInfo?.department?.deptName}')" href="#headingThree"><i class="fa fa-edit"></i></a>&nbsp;&nbsp;
                               <a class="btn btn-danger" onclick="RemoveDataApproval(${item.Id}, ${item.employeeId}, 'DeleteLeaveApproval')" href="javascript:void(0)"><i class="fa fa-trash-alt"></i></a>
							</td>
						</tr>`
    })
    $('#ApprovalTable tbody').empty();

    $('#ApprovalTable tbody').append(option);

    $('#tblitemDetails tbody').empty();
    $('#approvalTypeId').val(0)
    $('#statuses').val(0)
    $('#sortOrders').val('')
    $('#approverName').val('')

   }

