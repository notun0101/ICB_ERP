		}).then(function () {
			Common.Ajax('GET', '/HRPMSEmployee/FormView/DeleteNominee?id=' + Id + '&empId='+empId, [], 'json', ajaxDeleteNominee, false);
        });
        return false;
	}

	function ajaxDeleteNominee(res) {
		if (res == true) {
			swal('success', 'Delete Successfully', 'success').then(function () {
				location.reload();
			});
		}
		else {
			swal('warning', 'Delete Failed', 'warning');
		}
	}

