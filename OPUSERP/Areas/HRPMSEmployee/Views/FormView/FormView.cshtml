@model FormViewViewModelNew
@{
	ViewData["Title"] = "FormView";
}
@using System;
@using System.Globalization;
@section Style{
	<style>

		html {
			scroll-behavior: smooth;
		}

		html, body {
			overflow: scroll !important;
		}

		.redStar {
			color: red;
		}

		.opSelt .select2 {
			width: 84% !important;
		}

		.aa {
			cursor: pointer;
			background-color: #F8FEFF;
		}
		/*.aa:hover {
			background-color: #9ae2f3ed !important;
			color: white;
		}*/
		.input1 {
			font-size: 31px;
			font-weight: bold;
			border: none;
			background-color: transparent;
			width: 214px;
			text-align: center;
			outline-offset: 0px;
			outline: none;
		}

		.aaa {
			border-radius: 0 !important
		}

		.active {
			background-color: #9ae2f3ed !important;
			color: white;
		}

		/*Side Menu CSS*/
		#sideMenu {
			position: fixed;
			width: 100px;
			background-color: #293955;
			border-bottom-left-radius: 5px;
			border-top-left-radius: 5px;
			top: 90px;
			height: 580px;
			overflow-y: scroll;
			right: -90px;
			transition: 0.3s all;
			animation: sidemenu 2s infinite;
		}

			#sideMenu:hover {
				right: 0px;
				cursor: pointer;
				background-color: white;
			}

		.sideBtn {
			background: #D6DBE9;
			padding: 20px 5px;
			text-align: center;
			margin: 3px 0px;
		}

			.sideBtn a {
				color: black;
				text-decoration: none;
			}

				.sideBtn a:hover {
					color: #007ACC;
					text-decoration: none;
				}

		/*html {
			scroll-behavior: smooth;
		}*/

		.close {
			position: absolute;
			top: 15px;
			right: 35px;
			color: #f1f1f1;
			font-size: 40px;
			font-weight: bold;
			transition: 0.3s;
		}

			.close:hover,
			.close:focus {
				r color: #bbb;
				text-decoration: none;
				cursor: pointer;
			}

		#user_img1spouse:hover {
			border-radius: 5px;
			/*cursor: zoom-in;
			transition: 0.3s !important;
			opacity: 0.5;*/
			box-shadow: 0 0 2px 1px rgba(0, 140, 186, 0.5);
		}

		#myDIV {
			background-color: white;
			padding: 20px;
			box-shadow: 3px 3px 3px 3px lightgray;
			max-height: 415px;
			overflow-y: scroll;
			padding-top: 5px;
		}

		::-webkit-scrollbar {
			width: 10px;
			background-color: white;
		}

		::-webkit-scrollbar-thumb {
			background: #F3F3F3;
		}

		.g {
			text-decoration: none;
			color: #1492a5;
		}

			.g:hover {
				text-decoration: none;
				color: #070a67;
			}

			.g span {
				font-family: serif;
			}

		.small-box {
			text-align: center;
			border-radius: 10px;
			padding: 10px;
			transition: transform .5s;
		}

			.small-box:hover {
				background-color: #e8f0fe;
				transform: scale(.95);
			}

			.small-box i {
				font-size: 35px;
				margin: 10px;
			}





		/*Custom Css for Panel*/
		.panel {
			margin: 2px 0px;
		}

		.panel-heading {
			background: lightgray;
			padding: 10px;
			margin: 0;
		}

		.panel-title {
			margin: 0;
			font-size: 25px;
		}

			.panel-title a {
				text-decoration: none !important;
			}

				.panel-title a i {
					font-size: 20px;
				}

		.choosen {
			background: skyblue !important;
		}




		/* Works on Firefox */
		* {
			scrollbar-width: thin;
			scrollbar-color: blue orange;
		}

			/* Works on Chrome, Edge, and Safari */
			*::-webkit-scrollbar {
				width: 10px;
			}

			*::-webkit-scrollbar-track {
				background: lightgray;
			}

			*::-webkit-scrollbar-thumb {
				background-color: blue;
				border-right: 3px solid transparent;
			}
	</style>
