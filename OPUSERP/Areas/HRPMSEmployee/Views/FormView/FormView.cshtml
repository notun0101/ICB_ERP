}
<div class="card" style="width:100%">
	<div class="card-header row outBox" style="background-color: #EAF6FA;padding: 0; color: black;">
		<div class="col-md-4" style="display: flex; justify-content: flex-start; align-items: center; border-right: 1px solid #D9D8DB">
			<h5 class="card-title" style="color: black; margin: 0; text-transform: uppercase;">Employee's Information</h5>
		</div>
		<div class="col-md-8">
			@if (User.IsInRole("HRAdmin") || User.IsInRole("admin"))
			{
				<div style="float: left;">

					@if (Model.photograph?.url == null || Model.photograph?.url == "")
					{
						<img src="~/images/user-icon-png.png" data-toggle="modal" data-target="#updateEmpPhotoModal" style="cursor:pointer; margin-right: 5px;  border-radius: 50%;" title="Click to update photo." />

					}
					else
					{
						<img src="~/@Model.photograph?.url" data-toggle="modal" data-target="#updateEmpPhotoModal" style="cursor:pointer; margin-right: 5px;  border-radius: 50%;" title="Click to update photo." />

					}
					@*<img src="~/@Model.photograph?.url" data-toggle="modal" data-target="#updateEmpPhotoModal" style="cursor:pointer; margin-right: 5px;  border-radius: 50%;" title="Click to update photo." />*@
					<input type="hidden" id="hiddenFile" />
				</div>
			}
			else
			{
				<div style="float: left;">

					@if (Model.photograph?.url == null || Model.photograph?.url == "")
					{
						<img src="~/images/user-icon-png.png" style="cursor:pointer; margin-right: 5px;  border-radius: 50%;" title="Click to update photo." />

					}
					else
					{
						<img src="~/@Model.photograph?.url" style="cursor:pointer; margin-right: 5px;  border-radius: 50%;" title="Click to update photo." />

					}
					@*<img src="~/@Model.photograph?.url" data-toggle="modal" data-target="#updateEmpPhotoModal" style="cursor:pointer; margin-right: 5px;  border-radius: 50%;" title="Click to update photo." />*@
					<input type="hidden" id="hiddenFile" />
				</div>
			}
			<div style="float: left;">
				<div style="padding: 5px;text-align: center;">
					<p style="margin: 0; text-transform: uppercase;">@Model.employeeInfo?.nameEnglish</p>
					<p style="margin: 0">@Model.employeeInfo?.lastDesignation?.designationName</p>
				</div>
			</div>

			@if (User.IsInRole("UAT Admin"))
			{
				<div style="float: right; margin: 16px;">
					<a class='btn btn-info mr-4 float-right' style="" data-toggle='tooltip' title='Go Back' href='/HRPMSEmployee/Photograph/EditGrid/@Model.employeeInfo?.Id'><i class="fas fa-angle-double-left"> <span style="">Grid View</span></i></a>
				</div>
			}

		</div>
	</div>
	<div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
		<div class="panel panel-default">
			<div class="panel-heading" role="tab" id="headingOne">
				<h4 class="panel-title">
					<a role="button" data-toggle="collapse" data-parent="#accordion" href="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
						<i class="fas fa-chevron-right"></i>
						1. Personal Information
					</a>
				</h4>
			</div>
			<div id="collapseOne" class="panel-collapse collapse show" role="tabpanel" aria-labelledby="headingOne">
				<div class="panel-body">
					<div class="card-body">
						<div class="container">
							<form id="empInfoForm" parsley-validate="parsley">
								<div class="card">
									<div class="card-header">
										<h2 style="font-size:18px;color:black;"> Personal Information</h2>
									</div>
									<div class="card-body">
										<div class="col-12">
											<div class="row">
												<div class="col-6">
													<div class="form-group row">
														@*<label for="employeeCode" class="col-sm-4 col-form-label">@Model.fLang.employeeID <span class="redStar">*</span></label>*@
														<label for="employeeCode" class="col-sm-4 col-form-label">Employee Id<span class="redStar">*</span></label>
														<div class="col-sm-8">
															<input type="text" data-parsley-required="true" data-parsley-trigger="keyup" class="form-control" id="employeeCode" value="@Model.employeeInfo?.employeeCode" name="employeeCode">
															<input type="hidden" id="employeeID" name="employeeID" value="@Model.employeeID">
															<input type="hidden" name="activityStatus" id="activityStatus" value="@Model.employeeInfo?.activityStatus" />
															<input type="hidden" name="department" id="department" value="@Model.employeeInfo?.departmentId" />
															<input type="hidden" name="designation" id="designation" value="@Model.employeeInfo?.designation" />
															<input type="hidden" name="shiftGroupId" id="shiftGroupId" value="@Model.employeeInfo?.shiftGroupId" />
															<input type="hidden" name="isDelete" id="isDelete" value="@Model.employeeInfo?.isDelete" />

														</div>
													</div>
												</div>
												<div class="col-sm-6">
													<div class="form-group row">
														<label for="maritalStatus" class="col-sm-4 col-form-label">@Model.fLang.maritalStatus </label>
														<div class="col-sm-8">
															<select class="form-control" id="maritalStatus" name="maritalStatus">
																<option value="">Select</option>
																<option value="Married">Married</option>
																<option value="Unmarried">Unmarried</option>
																<option value="Widow">Widow</option>
																<option value="Separated">Separated</option>
															</select>
														</div>
													</div>
												</div>
											</div>

											<div class="row">
												<div class="col-6">
													<div class="form-group row">
														<label for="nameEnglish" class="col-sm-4 col-form-label">Employee Name (@Model.fLang.english)<span class="redStar">*</span></label>
														<div class="col-sm-8">
															<input type="text" data-parsley-required="true" data-parsley-trigger="keyup" class="form-control" id="nameEnglish" value="@Model.employeeInfo?.nameEnglish" name="nameEnglish" style="text-transform:uppercase">
														</div>
													</div>
												</div>
												<div class="col-sm-6">
													<div class="form-group row">
														<label for="bloodGroup" class="col-sm-4 col-form-label">@Model.fLang.bloodGroup</label>
														<div class="col-sm-8">
															<select class="form-control" id="bloodGroup" name="bloodGroup" value="@Model.employeeInfo?.bloodGroup">
																<option value="">Select</option>
																<option value="O-">O-</option>
																<option value="O+">O+</option>
																<option value="A-">A-</option>
																<option value="A+">A+</option>
																<option value="B-">B-</option>
																<option value="B+">B+</option>
																<option value="AB-">AB-</option>
																<option value="AB+">AB+</option>
															</select>
														</div>
													</div>
												</div>
											</div>
											<div class="row">
												<div class="col-6">
													<div class="form-group row">
														<label for="nameBangla" class="col-sm-4 col-form-label">Employee Name (@Model.fLang.bangla)</label>
														<div class="col-sm-8">
															<input type="text" class="form-control" id="nameBangla" value="@Model.employeeInfo?.nameBangla" name="nameBangla">
														</div>
													</div>
												</div>
												<div class="col-sm-6">
													<div class="form-group row">
														<label for="bloodDonateStatus" class="col-sm-4 col-form-label">Willingness to Blood Donation</label>
														<div class="col-sm-8">
															<select class="form-control" id="bloodDonateStatus" name="bloodDonateStatus" value="@Model.employeeInfo?.bloodDonateStatus">
																<option value="0">No</option>
																<option value="1">Yes</option>
															</select>
														</div>
													</div>
												</div>

											</div>
											<div class="row">
												<div class="col-sm-6">
													<div class="form-group row">
														<label for="mobileNumberPersonal" class="col-sm-4 col-form-label">Mobile(Personal)</label>
														<div class="col-sm-8">
															<input type="text" data-parsley-length="[11, 11]" data-parsley-length-message="This value should be exactly 11 digits long" data-parsley-type="digits" data-parsley-pattern="^[0-9]*$" data-parsley-trigger="keyup" class="form-control" id="mobileNumberPersonal" value="@Model.employeeInfo?.mobileNumberPersonal" name="mobileNumberPersonal" data-parsley-required="false">
														</div>
													</div>
												</div>

												<div class="col-sm-6">
													<div class="form-group row">
														<label for="telephoneResidence" class="col-sm-4 col-form-label">Telephone(Home)</label>
														<div class="col-sm-8">
															<input type="text" data-parsley-length="[6, 20]" data-parsley-trigger="keyup" class="form-control" id="telephoneResidence" value="@Model.employeeInfo?.telephoneResidence" name="telephoneResidence" data-parsley-required="false">
														</div>
													</div>
												</div>
											</div>
											<div class="row">
												<div class="col-sm-6">
													<div class="form-group row">
														<label for="birthIdentificationNo" class="col-sm-4 col-form-label">Birth Certificate No</label>
														<div class="col-sm-8">
															<input type="text" data-parsley-length="[10, 30]" data-parsley-type="digits" data-parsley-pattern="^[0-9]*$" data-parsley-trigger="keyup" class="form-control" id="birthIdentificationNo" value="@Model.employeeInfo?.birthIdentificationNo" name="birthIdentificationNo" data-parsley-required="false">
														</div>
													</div>
												</div>
												<div class="col-sm-6">
													<div class="form-group row">
														<label for="emailAddressPersonal" class="col-sm-4 col-form-label">Email (Personal)</label>
														<div class="col-sm-8">
															<input data-parsley-type="email" data-parsley-trigger="keyup" class="form-control" id="emailAddressPersonal" value="@Model.employeeInfo?.emailAddressPersonal" name="emailAddressPersonal">
														</div>
													</div>
												</div>
											</div>
											<div class="row">
												<div class="col-sm-6">
													<div class="form-group row">
														<label for="telephoneOffice" class="col-sm-4 col-form-label">Telephone(Official)</label>
														<div class="col-sm-8">
															<input type="text" data-parsley-length="[6, 20]" data-parsley-trigger="keyup" class="form-control" id="telephoneOffice" value="@Model.employeeInfo.telephoneOffice" name="telephoneOffice" data-parsley-required="false">
														</div>
													</div>
												</div>
												<div class="col-sm-6">
													<div class="form-group row">
														<label for="pabx" class="col-sm-4 col-form-label">PABX NO(Official)</label>
														<div class="col-sm-8">
															<input type="text" class="form-control" id="pabx" value="@Model.employeeInfo.pabx" name="pabx">
														</div>
													</div>
												</div>
											</div>
											<div class="row">
												<div class="col-sm-6">
													<div class="form-group row">
														<label for="emailAddress" class="col-sm-4 col-form-label">Email Address(Official)<span class="redStar">*</span></label>
														<div class="col-sm-8">
															<input class="form-control" required id="emailAddress" value="@Model.employeeInfo.emailAddress" name="emailAddress" data-parsley-trigger="keyup" data-parsley-required="true">
														</div>
													</div>
												</div>
												<div class="col-sm-6">
													<div class="form-group row">
														<label for="mobileNumberOffice" class="col-sm-4 col-form-label">Mobile Number(Official)</label>
														<div class="col-sm-8">
															<input type="text" data-parsley-length="[6, 20]" data-parsley-trigger="keyup" class="form-control" id="mobileNumberOffice" value="@Model.employeeInfo.mobileNumberOffice" name="mobileNumberOffice" data-parsley-required="false">
														</div>
													</div>
												</div>
											</div>
											<div class="row">
												<div class="col-sm-6">
													<div class="form-group row">
														<label for="homeDistrict" class="col-sm-4 col-form-label">@Model.fLang.homeDistrict</label>
														<div class="col-sm-8">
															<select class="form-control" id="homeDistrict" name="homeDistrict" value="@Model.employeeInfo?.homeDistrict">
																<option value="">Select</option>
																@foreach (var data in Model.districts)
																{
																	<option value="@data.districtName">@data.districtName</option>
																}
															</select>
														</div>
													</div>
												</div>
												<div class="col-sm-6">
													<div class="form-group row">
														<label for="birthPlace" class="col-sm-4 col-form-label">@Model.fLang.birthPlace </label>
														<div class="col-sm-8">
															<input type="text" class="form-control" id="birthPlace" value="@Model.employeeInfo?.birthPlace" name="birthPlace">
														</div>
													</div>
												</div>
											</div>
											<div class="row">
												<div class="col-6">
													<div class="form-group row">
														<label for="dateOfBirth" class="col-sm-4 col-form-label">@Model.fLang.dateOfBirth </label>
														<div class="col-sm-8">
															<input type="text" class="form-control datePicker" id="dateOfBirth" value="@Model.employeeInfo?.dateOfBirth?.ToString("dd-MMM-yyyy")" name="dateOfBirth" readonly>
														</div>
													</div>
												</div>
												<div class="col-sm-6 d-none">
													<div class="form-group row">
														<label for="tin" class="col-sm-4 col-form-label">@Model.fLang.tin</label>
														<div class="col-sm-8">
															<input type="text" data-parsley-length="[4, 25]" data-parsley-trigger="keyup" class="form-control" id="tin" value="@Model.employeeInfo?.tin" name="tin" data-parsley-required="false">
														</div>
													</div>
												</div>
												<div class="col-sm-6">
													<div class="form-group row">
														<label for="nationalID" class="col-sm-4 col-form-label">NID</label>
														<div class="col-sm-8">
															<input type="text" data-parsley-length="[10, 30]" data-parsley-type="digits" data-parsley-pattern="^[0-9]*$" data-parsley-trigger="keyup" class="form-control" id="nationalID" value="@Model.employeeInfo?.nationalID" name="nationalID" data-parsley-required="false">
														</div>
													</div>
												</div>
											</div>
											<div class="row">
												<div class="col-sm-6">
													<div class="form-group row">
														<label for="height" class="col-sm-4 col-form-label">Height (cm)  </label>
														<div class="col-sm-8">
															<input type="text" class="form-control" id="heightid" name="height" value="@Model.employeeInfo?.height">
														</div>
													</div>
												</div>
												<div class="col-sm-6 d-none">
													<div class="form-group row">
														<label for="PassportNo" class="col-sm-4 col-form-label">Passport No</label>
														<div class="col-sm-8">
															<input type="text" class="form-control" id="PassportNo" value="@Model.employeeInfo?.PassportNo" name="PassportNo" autocomplete="off">
														</div>
													</div>
												</div>
												<div class="col-sm-6">
													<div class="form-group row">
														<label for="identificationSign" class="col-sm-4 col-form-label">Identification Sign</label>
														<div class="col-sm-8">
															<input type="text" class="form-control" id="identificationSign" value="@Model.employeeInfo?.identificationSign" name="identificationSign">
														</div>
													</div>
												</div>
											</div>
											<div class="row">
												<div class="col-sm-6 d-none">
													<div class="form-group row">
														<label for="PassportIssueDate" class="col-sm-4 col-form-label">Passport Issue Date </label>
														<div class="col-sm-8">
															<input type="text" class="form-control datePicker" id="PassportIssueDate" name="PassportIssueDate" autocomplete="off" value="@Model.employeeInfo?.PassportIssueDate" readonly>
														</div>
													</div>
												</div>
												<div class="col-6 d-none">
													<div class="form-group row">
														<label for="PassportExDate" class="col-sm-4 col-form-label">Passport Expiration Date </label>
														<div class="col-sm-8">
															<input type="text" class="form-control datePicker" id="PassportExDate" name="PassportExDate" autocomplete="off" value="@Model.employeeInfo?.PassportExDate" readonly>
														</div>
													</div>
												</div>
												<div class="col-sm-6">
													<div class="form-group row">
														<label for="nationality" class="col-sm-4 col-form-label">@Model.fLang.nationality</label>
														<div class="col-sm-8">
