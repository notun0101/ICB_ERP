		        title: 'Are you sure?', text: 'Do you want to delete this record!', type: 'warning', showCancelButton: true, confirmButtonColor: '#3085d6', cancelButtonColor: '#d33', confirmButtonText: 'Yes, delete it!'
	        }).then(function () {
		        $.ajax({
			        url: '@Url.Action("DeleteChildren", "FormView")',
                    data: { id: Id, empId: employeeId },
			        type: 'GET',
		        })
		        .done(function () {
                    swal('', 'Deleted Successfully!', 'success').then(function () {
                        window.location.reload();
                    });
		        })
		        .fail(function () {
			        swal('', 'Unable to Delete! You have to delete Children Education.', 'warning');
		        });
	        });
    }
    function RemoveMobileBenefit(Id, employeeId) {
        chk = confirm('Do you want to delete this?');
