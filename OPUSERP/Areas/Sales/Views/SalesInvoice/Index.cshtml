                $.each(response, function (i, item) {
                    //var totalAmount = (item.itemPriceFixing.price * item.quantity) - ((item.itemPriceFixing.price * item.quantity) * item.itemPriceFixing.discountPersent / 100)
                    var trHtml = '<tr id=' + index + '>' +
