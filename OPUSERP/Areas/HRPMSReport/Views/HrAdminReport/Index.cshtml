        function ajaxGetDepartment(response) {
            var lstParticular = [];
            $.each(response, function (i, option) {
                var obj = new Object();
                obj.key = option.Id;
                obj.value = option.deptName;
                lstParticular[lstParticular.length] = obj;
            });
            $('#deptName').autocomplete({
                source: lstParticular,
                select: function (event, ui) {
                    $('#deptId').val(ui.item.key);
                    $("#deptName").val(ui.item.value);
                }
            });
        }

