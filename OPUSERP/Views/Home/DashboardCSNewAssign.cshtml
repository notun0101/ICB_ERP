<script type="text/javascript">
    // Load google charts
    google.charts.load('current', { 'packages': ['corechart'] });
    google.charts.setOnLoadCallback(drawChart);

    // Draw the chart and set the chart values
    function drawChart() {
        var data = google.visualization.arrayToDataTable([
            ['Task', 'Hours per Day'],
            ['', 3],
            ['', 8],
            ['', 2],
            ['', 3],
        ]);

        var options = { 'width': 250, 'height': 200 };

        var chart = new google.visualization.PieChart(document.getElementById('piechart'));
        chart.draw(data, options);
    }
    $(document).ready(function () {

        $('#PRopId').change(function () {
            var val = $(this).val();
            if (val == 1) {
                $('.PRBlock').hide();
                $('#PRDl').show();
            }
            else if (val == 2) {
                $('.PRBlock').hide();
                $('#PRDlM').show();
            }
            else {
                $('.PRBlock').hide();
                $('#PRDlY').show();
            }
        })

        $('#CSopId').change(function () {
            var val = $(this).val();
            if (val == 1) {
                $('.CSBlock').hide();
                $('#CSl').show();
            }
            else if (val == 2) {
                $('.CSBlock').hide();
                $('#CSM').show();
            }
            else {
                $('.CSBlock').hide();
                $('#CSY').show();
            }
        })

        $('#SPopId').change(function () {
            var val = $(this).val();
            if (val == 1) {
                $('.SPBlock').hide();
                $('#SPl').show();
            }
            else if (val == 2) {
                $('.SPBlock').hide();
                $('#SPM').show();
            }
            else {
                $('.SPBlock').hide();
                $('#SPY').show();
            }
        })

        $('#POopId').change(function () {
            var val = $(this).val();
            if (val == 1) {
                $('.POBlock').hide();
                $('#POl').show();
            }
            else if (val == 2) {
                $('.POBlock').hide();
                $('#POM').show();
            }
            else {
                $('.POBlock').hide();
                $('#POY').show();
            }
        })
    })

    function RAPending() {
        window.open('/SCMRequisition/RequisitionApprove/ReqApprovelist', '_blank');
    }
    function RAsPending() {
        window.open('', '_blank');
    }
    function CSAPending() {
        window.open('/SCMPurchaseProcess/PurchaseApprove', '_blank');
    }
    function SingleAPending() {
        window.open('/SCMPurchaseProcess/PurchaseApprove/IndexSingleOngoing', '_blank');
    }
    function POAPending() {
        window.open('/SCMPurchaseOrder/PurchaseOrder/POListForApprove', '_blank');
    }
    function SPAPending() {
        window.open('/SCMIOU/IOU/IOUListForApprove', '_blank');
    }
</script>