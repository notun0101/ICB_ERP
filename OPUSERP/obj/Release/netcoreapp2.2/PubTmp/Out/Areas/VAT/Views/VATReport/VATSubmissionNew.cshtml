
@{
    ViewData["Title"] = "VAT Submission";
    Layout = "~/Views/Shared/_PrintView.cshtml";
}

@section Style{
    <style>
        .print-card-body {
            width: 100%;
            padding-bottom: 10px;
        }

        .vartical-table th {
            width: 25%;
        }

        .vartical-table td {
            width: 25%;
        }

        .horizontal-table {
            /*width: 100%;*/
        }

        .table td, .table th {
            padding: .3rem;
            font-size: 15px;
        }

        table {
            page-break-inside: avoid;
            page-break-after: auto;
            page-break-before: avoid;
        }

        tr {
            page-break-inside: avoid;
            page-break-after: auto;
            page-break-before: avoid;
        }

        thead {
            display: table-header-group;
            page-break-inside: avoid;
            page-break-after: auto
        }

        tfoot {
            display: table-footer-group;
            page-break-inside: avoid;
            page-break-after: avoid;
        }

        h6 {
            font-size: 14px;
            background-color: gray;
            color: white !important;
            padding: 3px;
        }
    </style>
}
