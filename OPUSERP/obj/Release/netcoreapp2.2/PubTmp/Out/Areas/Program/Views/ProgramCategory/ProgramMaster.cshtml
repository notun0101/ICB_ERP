            $('#SubLedgerListI').append(' <div class="col-sm-4 row"><input type="hidden" name="partner[]" id="LgI' + id.replace(" ", "") + '" value="' + id + '"><span class="spanClass" id="LgIId' + id.replace(" ", "") + '" >' + id + '<a href="javascript:void(0)"><span class="crossButton" onclick="DeleteGroupI(' + "'" + id.replace(" ", "") + "'" + ')"><i class="fas fa-minus"></i></span></a></span></div>');
