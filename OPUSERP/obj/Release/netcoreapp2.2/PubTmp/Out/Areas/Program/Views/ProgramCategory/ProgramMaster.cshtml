            var id = 0;
            var count = 0;
            $('#programflow').find('textarea[type="hidden"]').each(function () {
                count += 1
                //alert(count);
                $('#Lg' + count + '').prop("disabled", true);
                $('#Lg' + count + '').prop("text-align", "justify");
            });
            id = count + 1;
            $('#programflow').append('<div id="Impact' + id + '" class="col-sm-4 row"><div class="col-sm-10 row"><textarea type="hidden" style="width:100%;border-color:blueviolet;" class="form-control"   id="Lg' + id + '"></textarea></div><div class="col-sm-2 row"><span class="spanClass" id="LgId' + id + '" ><a class="btn btn-success m-1" href="javascript:void(0)"><span class="crossButton" onclick="DeleteImpact(' + "'" + id + "'" + ')"><i class="fas fa-minus"></i></span></a><br/><a class="btn btn-success m-1" href="javascript:void(0)"><span class="crossButton" onclick="AddOutCome(' + id + ')"><i class="fas fa-plus"></i></span></a></span></div></div><div id="outcome' + id + '" class="col-sm-8 row"></div>');
