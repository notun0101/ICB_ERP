<div class="card" style="width: 100%;">
    <div class="card-header row" style="background-color:#EAF6FA;">
        <div class="col-8">
            <h5 class="card-title" style="color: black">Employee's Personal Information</h5>
        </div>
        <div class="col-4">
            <a class='btn btn-info mr-4 float-right' style="" data-toggle='tooltip' title='Go Back' href='/HRPMSEmployee/Info/AllEmployeeList'><i class="fas fa-angle-double-left"> <span style=""> Back</span></i></a>
            <a class='btn btn-info btn-md' style="margin-left:40%;background-color:skyblue" data-toggle='tooltip' href="/HRPMSEmployee/formview/formview?id=@Model.employeeInfo?.Id"> Full View</a>
            @*<h6 class="identity-box">@Model.employeeNameCode</h6>*@
        </div>
    </div>

    <div class="card-body row">
        <div class="col-2">

            @if (Model.photograph?.url == null || Model.photograph?.url == "")
            {
                <img id="user_img_current" style="width: 180px;height: 165px;border-radius: 120px;" src="/images/user-icon-png.png" alt="" class="img-thumbnail float-left">

            }
            else
            {
                <img id="user_img_current" style="width: 180px;height: 165px;border-radius: 120px;" src="/@Model.photograph?.url" alt="" class="img-thumbnail float-left">

            }
            <i class="fa fa-camera" data-toggle="modal" data-target="#updateEmpPhotoModal" style="position:relative;margin-top:120px;margin-left:-22px;cursor:pointer;"></i>
            <input type="hidden" id="hiddenFile" />
        </div>
        <div class="col-5" style="color: black; font-size: 15px; padding-top: 24px;">
            <input type="hidden" value="@ViewBag.employeeID" id="empId" />
            <span style="font-weight:bold;">Employee Id : </span><span>@Model.employeeInfo?.employeeCode</span><br />
            <span style="font-weight:bold;">Employee Name : </span><span>@Model.employeeInfo?.nameEnglish</span><br />
            <span style="font-weight:bold;">Designation : </span><span>@Model.employeeInfo?.lastDesignation?.designationName</span><br />
            <span style="font-weight:bold;">Contact : </span><span>@Model.employeeInfo?.mobileNumberPersonal</span><br />
            <span style="font-weight:bold;">Email : </span><span>@Model.employeeInfo?.emailAddress</span><br />
            <span style="font-weight:bold;">Present Posting :</span>
            @{
                var posting = "";
                var strings = new List<string>();
                foreach (var item in Model.employeePostings)
                {
                    if (!strings.Contains(item.placeName))
                    {
                        strings.Add(item.placeName);
                    }
                }
                posting = string.Join(",", strings);
            }
            <span>@posting</span>
            <br />
        </div>
        <div class="col-4 row" style="color: black; font-size: 15px;">
            @{
                var d = "";
                if (Model.photograph?.url != null)
                {
                    if (Model.photograph?.updatedAt != null)
                    {
                        d = Model.photograph?.updatedAt.ToString();
                    }
                    else
                    {
                        d = Model.photograph?.createdAt.ToString();
                    }
                }
                else
                {
                    if (Model.employeeInfo?.updatedAt != null)
                    {
                        d = Model.employeeInfo?.updatedAt.ToString();
                    }
                    else
                    {
                        d = Model.employeeInfo?.createdAt.ToString();
                    }
                }
                var exDate = Convert.ToDateTime(d).AddYears(5);
                var totalDays = (exDate - DateTime.Now).TotalDays;

                var totalYears = Math.Truncate(totalDays / 365);
                var totalMonths = Math.Truncate((totalDays % 365) / 30);
                var remainingDays = Math.Truncate((totalDays % 365) % 30);
            }
            @if (Model.photograph?.url != null)
            {
                <div class="col-md-12">
                    <div class="alert alert" style="background-color:#EAF6FA" role="alert">
                        You have to change photo after <span class="font-weight-bold">@totalYears years @totalMonths months and @remainingDays days</span>
                    </div>
                </div>
            }
            @*<div class="col-md-12">
                    <a target="_blank" href="/HRPMSEmployee/Info/FormView/@Model.employeeInfo?.Id" class="btn btn-info btn-block">Full View</a>
                </div>*@
        </div>
