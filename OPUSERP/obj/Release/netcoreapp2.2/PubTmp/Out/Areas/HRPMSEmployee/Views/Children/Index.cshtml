							<td><input type="hidden" value="${degree}" name="degreeId">${degree1}</td>
							<td><input type="hidden" value="${majorSubject}" name="majorSubject">${majorSubject}</td>
							<td><input type="hidden" value="${passingYear}" name="passingYear">${passingYear}</td>
							<td><input type="hidden" value="${resultType}" name="resultType">${resultType}</td>
							<td><input type="hidden" value="${result}" name="result">${result}</td>
							<td><a href="javascript:void(0)" onclick="RemoveRow(${passingYear})">X</a></td>
						</tr>`;

            $('#childEduTbl tbody').append(row);
            $('#degree').append('<option selected value="0">Select</option>');
           
            $('#institution').val('');
            $('#majorSubject').val('');
            $('#passingYear').val('');
            $('#resultType').val('');
            $('#result').val('');
        }

