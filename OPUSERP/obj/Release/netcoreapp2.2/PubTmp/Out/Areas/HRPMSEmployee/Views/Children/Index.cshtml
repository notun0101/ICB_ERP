            if ($("#degree").val() == null || $("#degree").val() == '') {
                alert('Degree required');
                return false;
            }
          
            if ($("#passingYear").val() == null || $("#passingYear").val() == '') {
                alert('Passing Year required');
                return false;
            }

            var institution = $('#institution').val();
            var degree = $('#degree :selected').val();
            var degree1 = $('#degree :selected').text();
            var majorSubject = $('#majorSubject').val();
            var passingYear = $('#passingYear').val();
            var resultType = $('#resultType').val();
            var result = $('#result').val();
            var levelofeducationName = $('#levelOfEducationId').val();
            var levelofeducationName1 = $('#levelOfEducationId :selected').text();

            var row = `<tr id="edu_${passingYear}">
							<td><input type="hidden" value="0" name="childEduId"><input type="hidden" value="${institution}" name="institution">${institution}</td>
