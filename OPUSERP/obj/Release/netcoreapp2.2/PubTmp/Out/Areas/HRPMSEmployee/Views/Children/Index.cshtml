<div class="row clearfix">
    <div class="col-12">
        <div class="card mb-4">
            <!-- Card Header - Dropdown -->
            <div class="card-header py-3 d-flex flex-row align-items-center justify-content-between">
                <h6 class="m-0 font-weight-bold">@Model.fLang.title</h6>
            </div>
            <!-- Card Body -->
            <div class="card-body">
                <table class="table table-striped table-bordered" id="childrenTable">
                    <thead>
                        <tr>
                            <th>@Model.fLang.childName</th>
                            <th>@Model.fLang.dateOfBirth</th>
                            <th>@Model.fLang.gender</th>
                            <th>Occupation</th>
                            @*<th>Designation</th>*@
                            @*<th>Organization</th>*@
                            <th>@Model.fLang.bloodGroup</th>
                            <th>Present Education</th>
                            <th>Picture</th>

                            @*<th>Image</th>*@
                            <th>@Model.fLang.action</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var data in Model.children)
                        {
                        <tr>
                            <td>@data.childName</td>
                            <td>@data.dateOfBirth?.ToString("dd-MMM-yyyy")</td>
                            <td>@data.gender</td>
                            <td>@data.occupation?.occupationName</td>
                            @*<td>@data.designation</td>*@
                            @*<td>@data.organization</td>*@
                            <td>@data.bloodGroup</td>
                            <td>@data.presentEducation</td>
                            @if (data.imageUrl == null)
                            {
                                <td></td>

                            }
                            else
                            {

                                <td> <a href="~/EmpImages/@data.imageUrl" target="_blank"><i class="fas fa-link"><u>Click Here</u></i></a></td>

                            }

                            <td>
                                <a class="btn btn-info" onclick="ViewEdu(@data.Id)" href="javascript:void(0)"><i class="fas fa-graduation-cap"></i></a>
                                <a class="btn btn-success" onclick="Edit(@data.Id,'@data.childName','@data.employeeId','@data.childNameBN','@data.dateOfBirth?.ToString("dd-MMM-yyyy")','@data.education','@data.gender','@data.occupation?.Id','@data.designation','@data.organization','@data.bin','@data.nid','@data.bloodGroup','@data.presentEducation','@data.disability','@data.disablityType','@data.imageUrl','@data.relationship','@data.emailAddressPersonal','@data.phone','@data.childNo','@data.maritalstatus')" href="#"><i class="fa fa-edit"></i></a>
                                <a class="btn btn-danger" onclick="Remove(@data.Id,@data.employeeId)" href="#"><i class="fa fa-trash-alt"></i></a>
                            </td>
                        </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

