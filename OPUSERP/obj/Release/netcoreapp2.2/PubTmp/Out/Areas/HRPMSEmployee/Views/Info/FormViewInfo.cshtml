                    console.log(e);
                    var srcUrl = e.target.result;
                    if (srcUrl.indexOf("application/pdf") > 0) {
                        $('#img1').attr('src', "/EmpAttachment/pdfIcon.png");
                        $('#userFile1').val(e.target.result);
                    }
                    else {
                        $('#img1').attr('src', e.target.result);
                    }

                }
                filerdr.readAsDataURL(input.files[0]);
            }
        }

