                                </div>
                                <div class="col-sm-6">
                                    <div class="form-group row">
                                        <label for="weight" class="col-sm-4 col-form-label">Weight (kg)</label>
                                        <div class="col-sm-8">
                                            <input type="text" class="form-control" id="weightid" value="@Model.employeeInfo?.weight" name="weight">
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-6">
                                    <div class="form-group row">
                                        <label for="gender" class="col-sm-4 col-form-label">@Model.fLang.gender</label>
                                        <div class="col-sm-8">
                                            <select class="form-control" id="gender" name="gender" value="@Model.employeeInfo?.gender">
