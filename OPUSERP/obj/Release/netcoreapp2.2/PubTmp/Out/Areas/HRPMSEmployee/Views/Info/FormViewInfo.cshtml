@model FormViewViewModel
@{
    ViewData["Title"] = "Full Form View";
    //Layout = "~/Views/Shared/_Layout.cshtml";
}
@section Style{
    <style>

        .redStar {
            color: red;
        }

        .opSelt .select2 {
            width: 84% !important;
        }

        .aa {
            cursor: pointer;
            background-color: #F8FEFF;
        }
        /*.aa:hover {
            background-color: #9ae2f3ed !important;
            color: white;
        }*/
        .input1 {
            font-size: 31px;
            font-weight: bold;
            border: none;
            background-color: transparent;
            width: 214px;
            text-align: center;
            outline-offset: 0px;
            outline: none;
        }

        .aaa {
            border-radius: 0 !important
        }

        .active {
            background-color: #9ae2f3ed !important;
            color: white;
        }



        /*Side Menu CSS*/
        #sideMenu {
            position: fixed;
            width: 100px;
            background-color: #293955;
            border-bottom-left-radius: 5px;
            border-top-left-radius: 5px;
            top: 90px;
            height: 580px;
            overflow-y: scroll;
            right: -90px;
            transition: 0.3s all;
            animation: sidemenu 2s infinite;
        }

            #sideMenu:hover {
                right: 0px;
                cursor: pointer;
                background-color: white;
            }

        .sideBtn {
            background: #D6DBE9;
            padding: 20px 5px;
            text-align: center;
            margin: 3px 0px;
        }

            .sideBtn a {
                color: black;
                text-decoration: none;
            }

                .sideBtn a:hover {
                    color: #007ACC;
                    text-decoration: none;
                }

        /*html {
            scroll-behavior: smooth;
        }*/

        .close {
            position: absolute;
            top: 15px;
            right: 35px;
            color: #f1f1f1;
            font-size: 40px;
            font-weight: bold;
            transition: 0.3s;
        }

            .close:hover,
            .close:focus {
                color: #bbb;
                text-decoration: none;
                cursor: pointer;
            }

        #user_img1:hover {
            border-radius: 5px;
            cursor: zoom-in;
            transition: 0.3s !important;
            opacity: 0.5;
            box-shadow: 0 0 2px 1px rgba(0, 140, 186, 0.5);
        }

        #myDIV {
            background-color: white;
            padding: 20px;
            box-shadow: 3px 3px 3px 3px lightgray;
            max-height: 415px;
            overflow-y: scroll;
            padding-top: 5px;
        }

        ::-webkit-scrollbar {
            width: 10px;
            background-color: white;
        }

        ::-webkit-scrollbar-thumb {
            background: #F3F3F3;
        }

        .g {
            text-decoration: none;
            color: #1492a5;
        }

            .g:hover {
                text-decoration: none;
                color: #070a67;
            }

            .g span {
                font-family: serif;
            }

        .small-box {
            text-align: center;
            border-radius: 10px;
            padding: 10px;
            transition: transform .5s;
        }

            .small-box:hover {
                background-color: #e8f0fe;
                transform: scale(.95);
            }

            .small-box i {
                font-size: 35px;
                margin: 10px;
            }
    </style>
    <link href="~/css/StyleSheet.css" rel="stylesheet" />
    <link href="~/Assets/formviewLoading.css" rel="stylesheet" />
}
<div class="loading">Loading&#8230;</div>

<div class="card" style="width: 100%;">
    <div class="card-header row outBox" style="background-color: #EAF6FA;padding: 0; color: black;">
        <div class="col-md-4" style="display: flex; justify-content: flex-start; align-items: center; border-right: 1px solid #D9D8DB">


            <h5 class="card-title" style="color: black; margin: 0; text-transform: uppercase;">Employee's Information</h5>
        </div>
        <div class="col-md-8">
            @if (User.IsInRole("HRAdmin") || User.IsInRole("admin"))
            {
                <div style="float: left;">

                    @if (Model.photograph?.url == null || Model.photograph?.url == "")
                    {
                        <img src="~/images/user-icon-png.png" data-toggle="modal" data-target="#updateEmpPhotoModal" style="cursor:pointer; margin-right: 5px;  border-radius: 50%;" title="Click to update photo." />

                    }
                    else
                    {
                        <img src="~/@Model.photograph?.url" data-toggle="modal" data-target="#updateEmpPhotoModal" style="cursor:pointer; margin-right: 5px;  border-radius: 50%;" title="Click to update photo." />

                    }
                    @*<img src="~/@Model.photograph?.url" data-toggle="modal" data-target="#updateEmpPhotoModal" style="cursor:pointer; margin-right: 5px;  border-radius: 50%;" title="Click to update photo." />*@
                    <input type="hidden" id="hiddenFile" />
                </div>
            }
            else
            {
                <div style="float: left;">

                    @if (Model.photograph?.url == null || Model.photograph?.url == "")
                    {
                        <img src="~/images/user-icon-png.png" style="cursor:pointer; margin-right: 5px;  border-radius: 50%;" title="Click to update photo." />

                    }
                    else
                    {
                        <img src="~/@Model.photograph?.url" style="cursor:pointer; margin-right: 5px;  border-radius: 50%;" title="Click to update photo." />

                    }
                    @*<img src="~/@Model.photograph?.url" data-toggle="modal" data-target="#updateEmpPhotoModal" style="cursor:pointer; margin-right: 5px;  border-radius: 50%;" title="Click to update photo." />*@
                    <input type="hidden" id="hiddenFile" />
                </div>
            }
            <div style="float: left;">
                <div style="padding: 5px;text-align: center;">
                    <p style="margin: 0; text-transform: uppercase;">@Model.employeeInfo?.nameEnglish</p>
                    <p style="margin: 0">@Model.employeeInfo?.designation</p>
                </div>
            </div>

            <div style="float: right; margin: 16px;">
                <a class='btn btn-outline-info' style="" data-toggle='tooltip' title='Go Back' href='/HRPMSEmployee/Photograph/EditGrid/@Model.employeeInfo?.Id'><i class="fas fa-backward"></i></a>
                @*<a class='btn btn-info' style="" data-toggle='tooltip' title='Print' target='_blank' href='/HRPMSEmployee/InfoView/pdspdf/@Model.employeeInfo?.Id'><i class='fa fa-print'></i></a>*@
                <a id="btngle" href="javascript:void(0)" class="btn btn-outline-secondary btn-sm"
                   style="position: absolute; right: 82px; top: 17px; box-shadow: none; font-size: 20px;"><i class="fas fa-th"></i></a>

            </div>
        </div>
    </div>
    <div class="card-body">
