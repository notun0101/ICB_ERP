@model EmployeeMarkingViewModel
@{
	ViewData["Title"] = "EmployeeMarkingReport";
	Layout = "~/Views/Shared/_PrintView.cshtml";
}

@section Style{
	<style>

		.print-card-body {
			width: 100%;
			/*padding-bottom: 10px;*/
		}

		.vartical-table th {
			width: 25%;
		}

		.vartical-table td {
			width: 25%;
		}

		.table td, .table th {
			padding: 2px 5px;
			/*font-size: 16px;*/
		}

		table {
			page-break-inside: auto;
		}

		tr {
			page-break-inside: avoid;
			page-break-after: auto;
		}

		thead {
			display: table-header-group;
		}

		tfoot {
			display: table-footer-group;
		}

		h6 {
			font-size: 14px;
			background-color: gray;
			color: white !important;
			padding: 3px;
		}
		body{
			font-size: 28px;
		}
	</style>
}

<div class="print-card" style="width: 100%;">
