
@{
    ViewData["Title"] = "JournalList";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<div class="row clearfix">
    <div class="col-12">
        <div class="card mb-4">

            @*<div class="card-header py-3 d-flex flex-row align-items-center justify-content-between">
            <h2 class="m-0 font-weight-bold text-primary">Journal List</h2>
        </div>*@


            <div class="card-body bg-white">
                <h5 class="m-0 font-weight-bold text-primary">Journal List</h5>
                <hr />

                <table class="table table-striped table-bordered table-hover bg-white" id="TblAcSummary">
                    <thead>
                        <tr>
                            <th>Journal No</th>
                            <th>Transaction Date</th>
                            <th>Transaction Details</th>
                            <th>Amount</th>
                            <th>Party Name</th>
                            <th>Project Name</th>
                            <th>Manage</th>

                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td>111</td>
                            <td>16-06-2021</td>
                            <td>Solid Note</td>
                            <td>25000</td>
                            <td>Bank ABC</td>
                            <td>First Project</td>
                            <td>
                                <a href="/ProvidentFund/PFAccounting/PreviewJournal">
                                    <i class="fa fa-eye" style="color:green"></i>
                                </a>
                                &nbsp;
                                <a href="/ProvidentFund/PFAccounting/EditJournal">
                                    <i class="fa fa-edit" style="color:green"></i>
                                </a>

                            </td>
                        </tr>

                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

<script>
    $(document).ready(function () {
        $('#TblAcSummary').DataTable();
    });
</script>
