                            <select id="selectYears" class="form-control" name="year"></select>
                        </div>

                        <label class="col-md-1 pt-1">Date</label>
                        <div class="col-md-2">
                            <input type="text" readonly name="date" value="@Model.bODWithCompanies.FirstOrDefault()?.chairmen?.boardOfDirector?.date?.ToString("dd-MMM-yyyy")" class="form-control" />
                        </div>
                        <div class="col-md-1">
                            <a href="" class="btn btn-success btn-sm"><i class="fas fa-backward"></i></a>
                        </div>


                    </div>
                </div>



            </div>

			<div class="card-body pb-0" id="raiserDiv" style="margin-left:400px;">
