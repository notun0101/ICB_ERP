								<input type="hidden" name="designation" id="designation1Inp" value="@Model.bODWithCompanies?.FirstOrDefault()?.chairmen?.boardOfDirector?.designation" />

								<input type="hidden" id="email" name="email" value="@Model.bODWithCompanies?.FirstOrDefault()?.chairmen?.boardOfDirector?.email" />
