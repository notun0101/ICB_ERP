        function OrderQTYKey(thisOQVal) {
            if (isNaN(thisOQVal) == true || thisOQVal == "") {
                thisOQVal = 0;
            }
            var orderQty = parseFloat($("#txtOrderedQty").val());
            if (orderQty > parseFloat($("#txtUnOrderedQty").val())) {
                swal('Attention!', "Order quantity must be less then unorder quantity", 'warning')
                $("#txtOrderedQty").val('');
            }
            else {
                VATA = parseFloat($('#txtVatAmount').val());
				vat = parseFloat($('#txtVat').val());
				$('#amontfield').val($('#txtUnitRate').val() * $('#txtOrderedQty').val())
				var amontfield = parseFloat($('#amontfield').val());
				var orderedQty = parseFloat($('#txtOrderedQty').val());

				URate = $('#txtUnitRate').val();

				var check = $('input[name="includeAlt"]:checked').val();
				var vat_amount = 0.0;
				var base_amount = 0.0;
				var total_amount = 0.0;
				if (check == '1' || check == '3') {
					console.log(amontfield + ' ' + vat);
					vat_amount = amontfield * (vat / 100);
					base_amount = (amontfield);
					total_amount = (base_amount + vat_amount);
					$('#txtVatAmount').val(vat_amount);
					$('#txtAmtExVat').val(base_amount);
					$('#txtTotalAmount').val(total_amount);
				}
				else if (check == '2') {
					console.log(amontfield + ' ' + vat);
					vat_amount = (amontfield * (1 - (vat / 100))) * (vat / 100);
					//vat_amount = (amontfield * (1 - (vat / 100))) / (vat / 100);
					base_amount = (amontfield - vat_amount);
					total_amount = (base_amount + vat_amount);
					$('#txtVatAmount').val(vat_amount);
					$('#txtAmtExVat').val(base_amount);
					$('#txtTotalAmount').val(total_amount);
				}
                //OQ = parseFloat(thisOQVal) * parseFloat(URate);
                //$('#txtAmtExVat').val(OQ).number(true, 2);
                if (isNaN(VATA)) {
                    VATA = 0;
                }
                //$('#txtTotalAmount').val(OQ + VATA).number(true, 2);
            }
